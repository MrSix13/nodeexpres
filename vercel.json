{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "env": {
                "NODE_ENV": "production",
                "scripts": {
                    "install": "npm install"
                }
            }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
  }